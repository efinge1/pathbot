  <!-- RESULTS evaluation based on highest trait score -->
  <category>
    <pattern>RESULTS</pattern>
    <template>
      <think>
        <set name="max">0</set>
        <set name="career">Undetermined</set>
        <condition name="problem_solver">
          <value><get name="problem_solver"/></value>
          <think>
            <set name="max"><get name="problem_solver"/></set>
            <set name="career">Software Developer</set>
          </think>
        </condition>
        <condition name="security">
          <value><get name="security"/></value>
          <think>
            <condition name="security">
              <value><eval><get name="security"/> &gt; <get name="max"/></eval></value>
              <set name="max"><get name="security"/></set>
              <set name="career">Cybersecurity Analyst</set>
            </condition>
          </think>
        </condition>
        <condition name="visual_thinker">
          <value><get name="visual_thinker"/></value>
          <think>
            <condition name="visual_thinker">
              <value><eval><get name="visual_thinker"/> &gt; <get name="max"/></eval></value>
              <set name="max"><get name="visual_thinker"/></set>
              <set name="career">Game Developer</set>
            </condition>
          </think>
        </condition>
        <condition name="technical">
          <value><get name="technical"/></value>
          <think>
            <condition name="technical">
              <value><eval><get name="technical"/> &gt; <get name="max"/></eval></value>
              <set name="max"><get name="technical"/></set>
              <set name="career">Network Administrator</set>
            </condition>
          </think>
        </condition>
        <condition name="analytical">
          <value><get name="analytical"/></value>
          <think>
            <condition name="analytical">
              <value><eval><get name="analytical"/> &gt; <get name="max"/></eval></value>
              <set name="max"><get name="analytical"/></set>
              <set name="career">Data Analyst</set>
            </condition>
          </think>
        </condition>
      </think>

      Based on your responses, your ideal career path is: <b><get name="career"/></b><br/>

      <condition name="career" value="Software Developer">
        Software Developers use logic and creativity to build apps and systems.<br/>
        <a href="https://www.bls.gov/ooh/computer-and-information-technology/software-developers.htm">Learn more</a>
      </condition>
      <condition name="career" value="Cybersecurity Analyst">
        Cybersecurity Analysts protect systems and respond to threats.<br/>
        <a href="https://www.bls.gov/ooh/computer-and-information-technology/information-security-analysts.htm">Learn more</a>
      </condition>
      <condition name="career" value="Game Developer">
        Game Developers design immersive and interactive digital experiences.<br/>
        <a href="https://money.usnews.com/careers/best-jobs/video-game-designer">Learn more</a>
      </condition>
      <condition name="career" value="Network Administrator">
        Network Administrators maintain and secure network infrastructure.<br/>
        <a href="https://www.bls.gov/ooh/computer-and-information-technology/network-and-computer-systems-administrators.htm">Learn more</a>
      </condition>
      <condition name="career" value="Data Analyst">
        Data Analysts turn numbers into decisions using logic and insight.<br/>
        <a href="https://www.usnews.com/education/learn-data-analysis-guide">Learn more</a>
      </condition>
    </template>
  </category>


